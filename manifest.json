{
  "manifest_version": 3,
  "name": "Corlius' Tab",
  "version": "1.0",
  "description": "Corlius' new tab page.",
  "permissions": ["tabs"],
  "chrome_url_overrides": {
      "newtab": "index.html"
  },
  "web_accessible_resources": [{
      "resources": [
          "icons/*",
          "trayicons/*",
          "wallpapers/*",
          "favicon.webp",
          "github.svg",
          "search.svg",
          "settings.png"
      ],
      "matches": ["<all_urls>"]
  }],
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  }
}